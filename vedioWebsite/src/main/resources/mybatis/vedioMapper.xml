<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="website.mapper.VedioMapper">

    <select id="vedioInfo" resultType="website.vo.VedioVo">
        Select USER_NAME,HEAD_IMG,URL,TITLE,DESCRIPTION,COVER_URL
        from vedio v,user u
        where v.USER_CODE = u.USER_CODE
    </select>

</mapper>